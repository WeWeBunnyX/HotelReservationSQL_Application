}

#iconOnlyWidget QPushButton {
    height: 50px;
    border: none;
    padding: 8px;
    background-color: transparent;
}

#iconOnlyWidget QPushButton:hover {
    background-color: rgba(200, 200, 200, 0.5);  /* Light gray hover */
}

#iconOnlyWidget QPushButton:checked {
    background-color: rgba(150, 150, 150, 0.5);  /* Medium gray for active state */
    border-right: 3px solid #70B9FE;  /* Blue accent for active state */
}

#iconOnlyWidget QLabel {
    padding: 5px;
    background-color: transparent;
}
/*= END
  ==================================================== */

/*= Style for full menu START (keep original dark version)
  ==================================================== */
#fullMenuWidget {
    background-color: #313a46;
    width: 200px;
}

#fullMenuWidget QPushButton {
    border: none;
    border-radius: 3px;
    text-align: left;
    padding: 8px 15px;
    color: #788596;
    height: 40px;
}

#fullMenuWidget QPushButton:hover {
    background-color: rgba(86, 101, 115, 0.5);
}

#fullMenuWidget QPushButton:checked {
    color: #fff;
    background-color: rgba(86, 101, 115, 0.8);
}

#fullMenuWidget QLabel#label_3 {
    color: #fff;
    font-size: 16px;
    padding: 10px;
}
/*= END
  ==================================================== */

/*= Update exit button border color for light sidebar
  ==================================================== */
#exitButton_1 {
    border-top: 1px solid #cccccc;  /* Lighter border color */
